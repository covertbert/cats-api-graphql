{
  "author": "Bertie Blackman",
  "description": "A GraphQL endpoint for the cats rest API",
  "devDependencies": {
    "apollo-server": "^2.15.1",
    "apollo-server-testing": "^2.15.1",
    "graphql": "^14.7.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "knex": "^0.21.5",
    "lint-staged": "^10.2.13",
    "node": "^14.8.0",
    "nodemon": "^2.0.4",
    "objection": "^2.2.3",
    "pg": "^8.3.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "keywords": [
    "GraphQL",
    "Cats",
    "API"
  ],
  "license": "ISC",
  "name": "cats-api-grapqhl",
  "nodemonConfig": {
    "exec": "npm run start",
    "ext": "ts, json",
    "watch": "src"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "all": "cd ops/docker; ./run.sh",
    "db": "cd ops/docker; ./run.sh db",
    "watch": "./ops/watch.sh",
    "lint": "tslint --project .",
    "lint:fix": "tslint --quiet --fix --project .",
    "prettier": "prettier --list-different \"src/**/*.ts\"",
    "test": "./ops/test.sh",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts",
    "migrate:run": "knex migrate:latest --knexfile knexfile.ts",
    "seed:make": "knex seed:make --knexfile knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts "
  },
  "version": "1.0.0",
  "dependencies": {
    "@types/jest": "^26.0.10"
  }
}
