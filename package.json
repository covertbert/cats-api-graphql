{
  "author": "Bertie Blackman",
  "description": "A GraphQL endpoint for the cats rest API",
  "devDependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-testing": "^2.9.4",
    "graphql": "^14.5.8",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "knex": "^0.19.5",
    "lint-staged": "^9.4.1",
    "node": "^12.11.0",
    "nodemon": "^1.19.3",
    "objection": "^1.6.11",
    "pg": "^7.12.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "keywords": [
    "GraphQL",
    "Cats",
    "API"
  ],
  "license": "ISC",
  "name": "cats-api-grapqhl",
  "nodemonConfig": {
    "exec": "npm run start",
    "ext": "ts, json",
    "watch": "src"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "all": "cd ops/docker; ./run.sh",
    "db": "cd ops/docker; ./run.sh db",
    "watch": "./ops/watch.sh",
    "lint": "tslint --project .",
    "lint:fix": "tslint --quiet --fix --project .",
    "prettier": "prettier --list-different \"src/**/*.ts\"",
    "test": "./ops/test.sh",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts -x ts",
    "migrate:run": "knex migrate:latest --knexfile knexfile.ts",
    "seed:make": "knex seed:make --knexfile knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts "
  },
  "version": "1.0.0",
  "dependencies": {
    "@types/jest": "^24.0.18"
  }
}
